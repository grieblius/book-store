{
  "name": "book-store",
  "version": "1.0.0",
  "description": "Default frontend package file",
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "clean-dist": "rimraf dist/*",
    "build": "npm run clean-dist & webpack --config=config/webpack.config.js --mode=production",
    "start": "node server/express.js",
    "dev": "webpack-dev-server --config=config/webpack.config.js --mode=development",
    "lint": "eslint ./src/**/*"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-typescript"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "settings": {
      "import/resolver": {
        "alias": {
          "map": [
            [
              "@components",
              "./src/components"
            ],
            [
              "@store",
              "./src/store"
            ],
            [
              "@utils",
              "./src/utils"
            ],
            [
              "@src",
              "./src"
            ]
          ],
          "extensions": [
            ".ts",
            ".tsx",
            ".js",
            ".jsx",
            ".json"
          ]
        }
      }
    },
    "ignorePatterns": [
      "*.ejs"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.45",
    "express": "^4.17.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "regenerator-runtime": "^0.13.3"

  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/classnames": "^2.2.9",
    "@types/node": "^13.7.7",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "babel-helper-evaluate-path": "^0.5.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-react-displayname": "0.0.5",
    "classnames": "^2.2.6",
    "core-js": "^3.6.4",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "fork-ts-checker-webpack-plugin": "^4.0.5",
    "html-webpack-plugin": "^3.2.0",
    "react-hot-loader": "^4.12.19",
    "redux-devtools-extension": "^2.13.8",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.10.3",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2"
  }
}
